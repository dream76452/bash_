pipeline-визуализированная последовательность этапов, через которые проходит проект или сделка от начальной стадии до завершения.
cd ../..=root dir
ctrl+u=delete
ctrl+w=delete last word
ls -la=ls -l -a
mkdir a\ b
rwx=421
cp копируемое куда
mv имя_изм на_что
echo "a">file restart
echo "b">>file append()
man=manual
info
chmod 777 file.txt
chown [опции] владелец[:группа] файл(ы)
sudo chown alice file.txt
find .
grep "error" file.txt
grep -n "error" file.txt
cat file.txt
curl
wget
ssh 
